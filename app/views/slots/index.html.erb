<div class="page-header">
  <h1>
    Slots
    <a href="/slots/new" class="btn btn-lg btn-success">New Slot</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Schedule</th>
        <th>Course</th>
        <th>Year</th>
        <th>Quarter</th>
        <th>Completed</th>
        <th>Grade</th>
        <th>Actions</th>
      </tr>

      <% @slots.each do |slot| %>
      <tr>
        <td><%= slot.schedule.user_id %></td>
        <td><%= slot.class.name %></td>
        <td><%= slot.year %></td>
        <td><%= slot.quarter %></td>
        <td><%= slot.completed %></td>
        <td><%= slot.grade %></td>
        <td>
          <a href="/slots/<%= slot.id %>" class="btn btn-primary">Show</a>
          <a href="/slots/<%= slot.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_slot/<%= slot.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
